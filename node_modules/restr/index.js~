var Restr = function () {
	this.data = {
		id: null,
		name: null,
		rank: null,
	};

	this.fill = function (info) {
		for(var prop in this.data) {
			if(this.data[prop] !== 'undefined') {
				this.data[prop] = info[prop];
			}
		}
	};

	this.getInformation = function () {
		return this.data;
	};
};

module.exports = function (info) {
	var instance = new Restr();
	instance.fill(info);
	return instance;
};
